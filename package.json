{
  "name": "fang",
  "description": "fang",
  "version": "1.0.0",
  "license": "MIT",
  "keywords": [
    "react",
    "isomorphic",
    "universal",
    "starter",
    "boilerplate",
    "template",
    "webpack",
    "koa",
    "transmit"
  ],
  "scripts": {
    "start": "npm run build && NODE_ENV=production babel-node ./backend/server.js",
    "dev": "NODE_ENV=development babel-node ./backend/server.js",
    "debug": "NODE_ENV=development babel-node-debug -p 8081 ./backend/server.js",
    "build-client": "webpack --colors --display-error-details --config configs/webpack.client.js",
    "build": "npm run build-client && cp -f frontend/index.html static/",
    "watch-client": "webpack-dev-server --config configs/webpack.client-watch.js"
  },
  "dependencies": {
    "chart.js": "^2.2.2",
    "isomorphic-fetch": "^2.2.1",
    "koa": "^1.2.2",
    "koa-proxy": "^0.6.0",
    "koa-router": "^5.4.0",
    "koa-static": "^2.0.0",
    "normalize.css": "^4.2.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-router": "^2.7.0",
    "mongoose": "^4.6.1",
    "koa-request": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "css-loader": "^0.24.0",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "node-sass": "^3.8.0",
    "sass-loader": "^4.0.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "koa-webpack-dev-middleware": "^1.2.1",
    "koa-webpack-hot-middleware": "^1.0.3",
    "webpack-dev-server": "^1.15.1",
    "webpack-hot-middleware": "^2.0.0",
    "webpack-node-externals": "^1.3.3"
  },
  "engines": {
    "node": ">=5.0.0"
  }
}